/* pages/wear/wear.wxss */
.wear {
  height: 100vh;
  overflow: hidden;
  width: 100%;
  background-color: #f5eaf8;
  position: absolute;
}
.info {
  position: absolute; top: 10rpx; right: 20rpx; width: 45rpx; height: 45rpx;
  border-radius: 50%;
  z-index:1;
}
.weather{
  margin: 60rpx 30rpx ;
  line-height: 55rpx;
  width: 60%;
  font-size: 30rpx;
  border-left: #b8a1e2 5px solid;
  /*background-image: linear-gradient(to right, #b8a1e2 , #f5eaf8);*/
  padding-left: 20rpx;
}

.top-image-tl {
  position: absolute;
  z-index: 2;
  top: 130rpx;
  width: 70%;
}
.top-image-rbt{ 
  position: absolute;
  z-index: 2;
  top: 20rpx;
  right: 10rpx;
  width: 50%;
}
.cloth{
  margin: 100rpx 50rpx 20rpx 50rpx;
  padding:  0 10rpx;
  line-height: 70rpx;
  font-size: 34rpx;
}

.cloth-card{ margin: 0rpx -50rpx; width: 700rpx; white-space: nowrap;}

/* 滑动区域 */
.swiper-item{ width: 550rpx; text-align: center;  margin:30rpx 75rpx; padding: 5rpx;}
.boxItem{ display: flex; text-align: center;  vertical-align: center; justify-content: center; align-items: center; width: 100%;  height: 100%;  padding: 0rpx; overflow-wrap: break-word; word-wrap: break-word; }

/* 翻转区域 */
.flipGroup{ width: 100%; perspective: 3500rpx;}
.flipBox{ width: 100%; backface-visibility: visible; transition: all 1s ease; transform-style: preserve-3d; }
.sideWrap{ width: 100%; height:100%; position: absolute; left: 0; top: 0; border-radius: 10rpx; overflow: hidden; }
.sideWrap.front{ border-radius: 15rpx;}
.sideWrap.back{ backface-visibility: visible; transform:rotateY(180deg); border-radius: 15rpx;}
.flipGroup.on .flipBox{ transform:rotateY(-180deg); }  


/* 搭配展示正面卡片 */
.frontGroup{ 
  width: 98%; height: 98%; text-align:left; border-radius: 15rpx; overflow:hidden; 
  /*border:1px solid #BFBFBF;*/
  box-shadow:2px 2px 2px #aaaaaa;
}
.bg-image {
  background-image: url('data:image/png;base64,');
}
.label {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  background-color: cornsilk;
  border-radius: 43%;
  padding: 15rpx 0;
  font-size: 30rpx;
  color: grey;
  letter-spacing: 5rpx;
  writing-mode: vertical-rl;
}

.wear-default {
  margin: 60rpx 30rpx;
  text-align: center;
  text-overflow: ellipsis;
  word-wrap: break-all; font-size: 28rpx;
  color: gray;
}
/*上下衣*/
.upper-wear1 { position: absolute; margin: 15% 8% 1vh; display: flex; width: 50%;}
.down-wear1 { position: absolute; display: flex; right:20rpx; top: 30vh; margin: 0 20rpx 1vh; width: 50%; }

/*裙子*/
.dress1 {
  position:absolute; display: flex; margin: 25% 20% 1vh; width: 50%;}
.dress2 {
  position:absolute; display: flex; margin: 10% 40rpx 1vh; width: 50%;}

/*外套+上下衣*/
.outer-wear { position: absolute; top: 40vh; width:305rpx; margin: 0vh 0rpx 0vh; display: flex; width: 305rpx; height: 280rpx; }
.upper-wear2 { position: absolute; margin: 3vh 5rpx 1vh; display: flex; width: 305rpx; height: 280rpx; }
.down-wear2 { position:absolute; top: 20vh; right: 0; display: flex;width: 310rpx; height: 280rpx; }
  
/* backGroup */
.backGroup{ width: 98%; height: 98%; text-align:left; border-radius: 15rpx; overflow:hidden; 
  /*border:1px solid #BFBFBF;*/
  background-color:rgb(255, 217, 224);
  box-shadow:2px 2px 2px #aaaaaa;text-align: left; font-size: 28rpx; }

  .tips {
    position: sticky;
    margin: 30rpx;
    text-align: center;
    text-overflow: ellipsis;
    word-wrap: break-all;
  }

  /*工具栏*/
  .tools {
    position: absolute;
    top: 43%;
    right: 20rpx;
    width: 54rpx;
    height: 18vh;
    padding: 25rpx 0 0 8rpx;
    border-radius: 30rpx;
    background: #8865c9;
    opacity: 0.5;
  }